<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>
@foreach (Dictionary<string, string> job in ViewBag.jobs)
{
    <table>
        @foreach (KeyValuePair<string, string> entry in job)
        {
            <tr>
                <td>@entry.Key
                <td></td>
                <td>@entry.Value</td>
            </tr>
        }
    </table>
        <br />
        <br />
        <br />
}
 